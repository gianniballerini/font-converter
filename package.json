  "main": "src/main/main.js",
  "repository": "git@github.com:gianniballerini/font-converter.git",
  "author": "Gianni Ballerini <gianniballerini@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "archiver": "^7.0.1",
    "electron": "^36.1.0",
    "woff2-encoder": "^2.0.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "pug": "^3.0.3",
    "sass": "^1.87.0",
    "vite": "^6.3.4",
    "vite-plugin-pug-transformer": "^1.0.5",
    "wait-on": "^8.0.3"
  },
  "scripts": {
    "dev": "concurrently -k \"vite\" \"wait-on http://localhost:1234 && cross-env VITE_DEV_SERVER_URL=http://localhost:1234 electron .\"",
    "build": "vite build",
    "start-vite": "vite --host --port 1234",
    "start": "electron ."
  }
